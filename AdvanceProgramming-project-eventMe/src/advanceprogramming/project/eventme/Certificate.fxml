            <Button id="buttonsofAttend" fx:id="buttonofAttendinAttend" alignment="CENTER" contentDisplay="CENTER" layoutX="90.0" layoutY="147.0" mnemonicParsing="false" onAction="#buttonofSendinCert" prefHeight="40.0" prefWidth="120.0" stylesheets="@styleSheet.css" text="SEND">
